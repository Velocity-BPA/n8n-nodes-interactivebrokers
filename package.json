{
  "name": "n8n-nodes-interactivebrokers",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Interactive Brokers providing trading, account management, market data, and portfolio operations via the Client Portal API.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-interactivebrokers.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-interactivebrokers#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-interactivebrokers/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "interactive-brokers",
    "ibkr",
    "trading",
    "stocks",
    "options",
    "futures",
    "forex",
    "bonds",
    "portfolio",
    "market-data",
    "algorithmic-trading",
    "automation"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/InteractiveBrokersApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/InteractiveBrokers/InteractiveBrokers.node.js",
      "dist/nodes/InteractiveBrokers/InteractiveBrokersTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && npm run copy:icons",
    "copy:icons": "copyfiles -u 1 \"nodes/**/*.svg\" \"nodes/**/*.png\" dist/",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.35.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
